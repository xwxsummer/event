/*top*/
.myGoods_content{
  box-sizing: border-box;
  padding-left: 1.6rem;
  padding-right: 1.6rem;
  margin-bottom: 10px;
  padding-top: 10px;
}
.myGoods_jiantou{
  width: 7rem;
  display: inline-block;
  vertical-align: bottom;
}
.myGoods_bottom{
  height: 2.6rem;
  line-height: 2.6rem;
}
.jiantou_7{
  top: -7px;
}
.myGoods_cancel{
  height: 1.6rem;
  line-height: 1.6rem;
  border: 1px solid #4bbcbd;
  color: #4bbcbd;
  border-radius: 10px;
  width: 4.7rem;
  margin-top: 0.5rem;
  margin-left: 5px;
}
div .myGoods_content:last-child{
  margin-bottom: 5rem;
}

/*取消*/
.mui-popover.mui-popover-action .mui-table-view {
  margin: 0;
  text-align: center;
  color: #999;
  border-radius: 0px;
}
.cancle_resuit{
  background-color: rgba(204, 204, 204, 1);
  height: 3rem;
  line-height: 3rem;
  color: #000;
}
.cancel_ul{
  padding-top: 20px;
}
.cancel_li{
  border: 1px solid #000;
  border-radius: 15px;
  width: 40%;
  display: inline-block;
  margin-bottom: 10px;
  height: 2.5rem;
  text-align: center;
  margin-left: 10px;
  margin-right: 10px;
  color: #000;
}
.cancle_yes,.cancle_no{
  border: none;
  border-radius: 5px;
  width: 40%;
  display: inline-block;
  margin-bottom: 20px;
  height: 2.2rem;
  line-height: 2.2rem;
  text-align: center;
  margin-left: 10px;
  margin-right: 10px;
  color: #000;
}
.bgc_c0c0{
  background-color: #c0c0c0;
}

.mgGoodsList div{
  line-height: 1.4rem

}
.special{
  border: 1px solid #ea5529;
  color: #ea5529;
}
.cancel_ul a{
  display: inline-block;
  width: 100%;
}
/*滑动*/
.mui-scroll-wrapper{
  margin-top: 3.6rem;
  margin-bottom: 4.6rem;
}
.moreInfo{
  margin-bottom: 3rem;
}
.moreInfo img {
  width: 20px;
  vertical-align: middle;
}
.bg{
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,.4);
  z-index:998;
}
.ceng_show{
  display: block;
  position: absolute;
  top: 20%;
  left: 10%;
  min-height: 273px;
  width: 80%;
  background-color: rgba(255,255,255,.95);
  z-index:1002;
  font-size: 14px;
  color: #000;
  font-weight: 900;
  padding: 40px 40px;
}
#contact_show ul li{
  height: 2.4rem;
  line-height: 2.4rem;
  text-align: center;
  border-radius: 10px;
  margin: 25px 0;
  border: 1px solid #333;
  color: #333;
}
#contact_show ul li:last-child{
  margin-bottom: 10px;
}
#contact_show .cancel_span{
  top: 10px;
  right: 10px;
  font-size: 26px;

}
.contactUs_cancel,.contactUs_sure{
  width: 50%; height: 44px;line-height:44px;font-size: 17px;
}
.mui-radio input[type='radio']:before, .mui-checkbox input[type='checkbox']:before{
  font-size: 1.5rem;
}
.mui-radio.mui-left input[type='radio'], .mui-checkbox.mui-left input[type='checkbox']{
  left: 0px;
}
.mui-radio.mui-left label, .mui-checkbox.mui-left label{
  padding-left: 2.2rem;
}
.padding22{
  padding-left: 2.2rem;
  margin-bottom: 10px;
}
.mui-radio input[type='radio'], .mui-checkbox input[type='checkbox']{
  top: 10px;
}
.col_999{
  width: 5rem;
}