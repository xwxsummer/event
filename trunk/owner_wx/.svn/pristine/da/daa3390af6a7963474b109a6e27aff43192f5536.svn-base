<div class="mui-scroll-wrapper mui-slider-indicator mui-segmented-control mui-segmented-control-inverted">
  <div class="mui-scroll">
    <div class="mui-control-item mui-active text_center" (click)="orderStatus(null)">全部</div>
    <div class="mui-control-item text_center" (click)="orderDataForPay(105)">待收货</div>
    <div class="mui-control-item text_center" (click)="orderDataing(106)">已收货</div>
    <div class="mui-control-item text_center" (click)="orderFinish(104)">协商中</div>
  </div>
</div>

<div class="mui-scroll-wrapper" style="margin-top: 53px;">
  <div class="mui-scroll" id="info">
    <!--全部-->
    <div *ngIf="isAll==1">
        <div class="Lists b-t b-b" (click)="receiptDetails(site.childNo)" *ngFor="let site of data">
          <div class="col_333 fon900 height36 back_fff padding17">
            <img src="{{site.headImg}}" class="touxiang" alt="" (click)="driverdetails(site.driverId);$event.stopPropagation();"/>
            &nbsp;&nbsp;<span class="fonSml">{{site.driverName}}</span>&nbsp;&nbsp;
            <span class="perTopSpan font12 fff">&nbsp;<img src="../assets/images/jifen.png" alt=""/>&nbsp;&nbsp;&nbsp;已认证</span>
            <span class="pull-right ea5529" *ngIf="site.status==2">接单成功</span>
            <span class="pull-right ea5529" *ngIf="site.status==3">前往货源地</span>
            <span class="pull-right ea5529" *ngIf="site.status==4">到达货源地</span>
            <span class="pull-right ea5529" *ngIf="site.status==50">货主确认车辆</span>
            <span class="pull-right ea5529" *ngIf="site.status==51">货主装车</span>
            <span class="pull-right ea5529" *ngIf="site.status==52">司机确认装车</span>
            <span class="pull-right ea5529" *ngIf="site.status==53">前往目的地</span>
            <span class="pull-right ea5529" *ngIf="site.status==54">到达目的地</span>
            <span class="pull-right ea5529" *ngIf="site.status==100">货主确认车辆</span>
            <span class="pull-right ea5529" *ngIf="site.status==101">货主确认收货</span>
            <span class="pull-right ea5529" *ngIf="site.status==102">司机确认收货</span>
            <span class="pull-right ea5529" *ngIf="site.status==150">完成</span>
            <span class="pull-right ea5529" *ngIf="site.status==151">评价完成 </span>
            <span class="pull-right ea5529" *ngIf="site.status==200">货主拒绝装车</span>
            <span class="pull-right ea5529" *ngIf="site.status==210">货主取消</span>
            <span class="pull-right ea5529" *ngIf="site.status==211">司机取消</span>
            <span class="pull-right ea5529" *ngIf="site.status==212">客服取消</span>
            <span class="pull-right ea5529" *ngIf="site.status==220">协商中</span>
          </div>
          <div class="dis_flex padding17">
            <ul class="clearfix fon85">
              <li class="">车牌号：{{site?.driverName }}</li>
              <li class="">载重：{{site?.truckLoad }}吨</li>
              <li class="">车型：{{site?.truckModel }}</li>
              <li class="">实际载货：{{site?.sendNet }}吨</li>
            </ul>
            <div><span class="col_999">></span></div>
          </div>
          <div class="jiedanshu_bottom back_fff padding17">
            <span class="jiedanshu_contact pull-right text_center" (click)="trajectoryMap(site.childNo)" *ngIf="site.status==2||site.status==3||site.status==4||site.status==50||site.status==51||site.status==52||site.status==53||site.status==54||site.status==100||site.status==101||site.status==102||site.status==150||site.status==151">轨迹查询</span>
            <span class="jiedanshu_contact pull-right text_center">联系司机</span>
          </div>
        </div>
       <div (click)="moreInfo()" class="moreInfo text_center fonSml" *ngIf="!isLoading">点击加载更多</div>
       <div (click)="moreInfo()" class="moreInfo text_center fonSml" *ngIf="isLoading">正在加载 <img src="../assets/images/loading.jpg" alt=""/></div>
    </div>
    <!--部分-->
    <div *ngIf="isAll!=1">
      <!--待收货-->
      <div *ngIf="myloading==105">
        <div class="Lists b-t b-b" (click)="receiptDetails(site.childNo)" *ngFor="let site of dataForPay">
          <div class="col_333 fon900 height36 back_fff padding17">
            <img src="{{site.headImg}}" class="touxiang" alt="" (click)="driverdetails(site.driverId);$event.stopPropagation();"/>
            &nbsp;&nbsp;<span class="fonSml">{{site.driverName}}</span>&nbsp;&nbsp;
            <span class="perTopSpan font12 fff">&nbsp;<img src="../assets/images/jifen.png" alt=""/>&nbsp;&nbsp;&nbsp;已认证</span>
            <span class="ea5529 pull-right fonSml">待收货</span>
          </div>
          <div class="dis_flex padding17">
            <ul class="clearfix fon85">
              <li class="">车牌号：{{site?.driverName }}</li>
              <li class="">载重：{{site?.truckLoad }}吨</li>
              <li class="">车型：{{site?.truckModel }}</li>
              <li class="">实际载货：{{site?.sendNet }}吨</li>
            </ul>
            <div><span class="col_999">></span></div>
          </div>
          <div class="jiedanshu_bottom back_fff padding17">
            <span class="jiedanshu_contact pull-right text_center">轨迹查询</span>
            <span class="jiedanshu_contact pull-right text_center">联系司机</span>
          </div>
        </div>
      </div>
      <!--已收货-->
      <div *ngIf="myloading==106">
        <div class="Lists b-t b-b" (click)="receiptDetails(site.childNo)" *ngFor="let site of dataing">
          <div class="col_333 fon900 height36 back_fff padding17">
            <img src="{{site.headImg}}" class="touxiang" alt="" (click)="driverdetails(site.driverId);$event.stopPropagation();"/>
            &nbsp;&nbsp;<span class="fonSml">{{site.driverName}}</span>&nbsp;&nbsp;
            <span class="perTopSpan font12 fff">&nbsp;<img src="../assets/images/jifen.png" alt=""/>&nbsp;&nbsp;&nbsp;已认证</span>
            <span class="ea5529 pull-right fonSml">已收货</span>
          </div>
          <div class="dis_flex padding17">
            <ul class="clearfix fon85">
              <li class="">车牌号：{{site?.driverName }}</li>
              <li class="">载重：{{site?.truckLoad }}吨</li>
              <li class="">车型：{{site?.truckModel }}</li>
              <li class="">实际载货：{{site?.sendNet }}吨</li>
            </ul>
            <div><span class="col_999">></span></div>
          </div>
          <div class="jiedanshu_bottom back_fff padding17">
            <span class="jiedanshu_contact pull-right text_center" (click)="trajectoryMap(site.childNo)">轨迹查询</span>
            <span class="jiedanshu_contact pull-right text_center">联系司机</span>
          </div>
        </div>
      </div>
      <!--协商中-->
      <div *ngIf="myloading==104">
        <div class="Lists b-t b-b" (click)="receiptDetails(site.childNo)" *ngFor="let site of dataFinish">
          <div class="col_333 fon900 height36 back_fff padding17">
            <img src="{{site.headImg}}" class="touxiang" alt="" (click)="driverdetails(site.driverId);$event.stopPropagation();"/>
            &nbsp;&nbsp;<span class="fonSml">{{site.driverName}}</span>&nbsp;&nbsp;
            <span class="perTopSpan font12 fff">&nbsp;<img src="../assets/images/jifen.png" alt=""/>&nbsp;&nbsp;&nbsp;已认证</span>
            <span class="ea5529 pull-right fonSml">协商中</span>
          </div>
          <div class="dis_flex padding17">
            <ul class="clearfix fon85">
              <li class="">车牌号：{{site?.driverName }}</li>
              <li class="">载重：{{site?.truckLoad }}吨</li>
              <li class="">车型：{{site?.truckModel }}</li>
              <li class="">实际载货：{{site?.sendNet }}吨</li>
            </ul>
            <div><span class="col_999">></span></div>
          </div>
          <div class="jiedanshu_bottom back_fff padding17">
            <!--<span class="jiedanshu_contact pull-right text_center">轨迹查询</span>-->
            <span class="jiedanshu_contact pull-right text_center">联系司机</span>
          </div>
        </div>
      </div>
      <div (click)="moreInfo()" class="moreInfo text_center fonSml" *ngIf="!isLoading">点击加载更多</div>
      <div (click)="moreInfo()" class="moreInfo text_center fonSml" *ngIf="isLoading">正在加载 <img src="../assets/images/loading.jpg" alt=""/></div>
    </div>
  </div>
</div>
<div class="text_center m-t3" id="infoNone">暂无该数据</div>