#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_@angular_compiler-cli@4.1.3@@angular/compiler-cli/src/main.js" "$@"
  ret=$?
else 
  node  "$basedir/../_@angular_compiler-cli@4.1.3@@angular/compiler-cli/src/main.js" "$@"
  ret=$?
fi
exit $ret
