.height3{
    height: 2.5rem;
    line-height: 2.5rem;
}
.padding16{
    padding-left: 1.6rem;
    padding-right: 1.6rem;
    box-sizing: border-box;
}
.myGoods_content{
    box-sizing: border-box;
    padding-left: 1.6rem;
    padding-right: 1.6rem;
    margin-bottom: 10px;
}
.huwu_jian{
    width: 2.4rem;
    display: inline-block;
    vertical-align: bottom;
}
.myGoods_bottom{
    height: 2.6rem;
    line-height: 2.6rem;
}

.re_b{
    height: 7rem;
    display: flex;
}
.re_l{
    width: 30%;
    height: 7rem;
    overflow:hidden;
    zoom:1;
}
.re_l nav{
    padding-left: 0.4rem;
}
.re_r{
    padding-top: 10px;
    flex: 1;
}
.big{
    margin: 2rem auto;
    height: 50px;
   width: 100%;
    margin: 0 auto;
}
.box{
    margin: 2rem auto;
    width:50%;
    height: 50px;    
}
.box input{
    background: #ea5529;
    color: #ffffff;
    border-radius: 8px;
    /*margin-left: 10%;*/
    width: 40%;
    height: 2rem;
    margin-bottom: 1rem;

}
.box input:nth-child(2){
    float: right;
}
.headImg{
  width:3.5rem;
  height:3.5rem;
  border-radius: 50%;
}
