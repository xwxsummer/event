
<div class="myGoods_content back_fff m-b10 ">
    <div class="height3 fonSml fon900 col_333 b-b pL_10">货物详情</div>
    <div class="b-b">
        <div class="m-t10"></div>
        <div class="pR_10 pL_10 m-b10 mgGoodsList">
            <div>出发地：{{data?.sendProvince}}</div>
            <div>目的地：{{data?.receiveProvince}}</div>
            <div>货物名称：{{data?.goodsName}}</div>
            <div>货物数量：{{data?.goodsAmount.toFixed(2)}}/{{data?.goodsUnit}}</div>
            <div>运费单价：{{data?.freightPrice.toFixed(2)}}元/吨</div>
            <div>到达货源地时间：{{data?.arriveTime}}</div>
            <div>装货时间：{{data?.loadTime}}</div>
            <div>卸货时间：{{data?.unloadTime}}</div>
        </div>
    </div>
</div>