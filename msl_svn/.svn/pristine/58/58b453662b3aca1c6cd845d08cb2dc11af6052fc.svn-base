
.select_trangle{
  width: 13px;
  vertical-align: middle;
}
.market_nav_top{
  width: 33.3%;
}
.height_28{
  height: 2.8rem;
  line-height: 2.8rem;
}
.market_main{
  margin: 5px;
  height: 14rem;
  border-radius: 10px;
  border: 1px solid #CCC;
}
.data{
	height: 13rem;
	border: 1px solid #E5E5E5;
	/*border-bottom: 5px solid #ea5529;*/
	margin: 0.6rem;
	border-radius: 5px;
	background: #ffffff;

}
.data:nth-child(1){
	margin-top: 2rem;
}
.data .data_t{
	width: 80%;
	height: 15%;
	background:url(/assets/images/jiantou.png) no-repeat 6.8rem 1.1rem ;
	/*5.6rem 1rem*/
	margin: 0 auto;
	background-size:5rem;
}
.data .data_t h3{
	color: #333333;
	font-size: 1rem;
}
.data .data_t h3:nth-child(1){
	float: left;
}
.data .data_t h3:nth-child(2){
	float: right;
}
.data .data_t nav{
	/*margin-left: 48%;*/
	text-align: center;
	padding-top: 0.4rem;
}

.data .left{
	width: 35%;
	height: 8rem;
	margin-top: 10PX;
	float: left;
	position: relative;

}
.data .right{
	width: 65%;
	height: 8rem;
	float: right;
	margin-top: 10PX;
	position: relative;

}
.header{
	width: 110px;
	height: 130px;

}

.pull-left .header .img{
	display: block;
	width: 4rem;
	height: 4rem;
	/*background: url("/assets/images/lunbotu.png") ;*/
	margin-left:0.75rem;
	/*border-radius: 40px;*/
	margin-top: 0.6rem;
	border: 1px solid #E5E5E5;
	border-radius: 50%;

}
.pull-left .header .a{
	display: block;
	width: 45px;
	height: 20px;
	background: url("/assets/images/renzheng.png") no-repeat center;
	background-size: 2.75rem;
	font-size: 0.75rem;
	color: #ffffff;
	font-weight: 700;
	position: absolute;
	left: 1.3rem;
	text-align: center;
}
.pull-left .header .a2{
	position: absolute;
	width: 90%;
	text-align: center;
	margin-top: 20px;
}
.data .right span{
	display: inline-block;
	color: #999999;
	font-size: 0.8rem;

}
.data .right nav{
	width: 3.2rem;
	height: 2rem;
	float: right;
	position: absolute;
	left: 8rem;
	top:1.8rem;
	text-align: right;
	line-height: 2rem;
	border-left:1px solid #E5E5E5;
	color: #999999;
}
.data .a_left{
	float: left;
	color: #8c8c8c;
}
.data .a_right{
	float: right;
	color: #8c8c8c;
}
.mui-scroll-wrapper{
  margin-top: 15.5rem;
  margin-bottom: 3.5rem;
}
#scrollBottom{
  margin-bottom: 5rem;
}
.top{
	padding-top: 1.2rem;
	padding-left: 1rem;
}
.top2{
	padding-top: 1.2rem;
}
.mui-slider .mui-slider-group .mui-slider-item > a:not(.mui-control-item){
	margin-top: 1.2rem;
}


