.mui-content{
    position: fixed;
    width: 100%;
    bottom: 0px;
}
table#number_input {
    background-color: white;
    width: 100%;
    text-align: center;

}

table#number_input td {
    border: 1px solid gainsboro;
    border-right: 0;
    width: 10%;
    height: 40px;
    padding-top: 0.7rem;
    font-size: 1rem;
}

table#number_input td:last-child {
    border-right: 1px solid gainsboro;
}

table#keyboard {
    /*position: fixed;*/
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
    width: 100%;
    height: 160px;
    background-color: white;
}

table#keyboard td {
    border: 1px solid gainsboro;
    border-right: 0;
    border-bottom: 0;
    width: 33%;
    height: 40px;
}

table#keyboard td:enabled {
    color: red;
}

table#keyboard td:last-child {
    border-right: 1px solid gainsboro;
}

.forgetPassword{
    color: #0062CC;
}
.mui-popover.mui-popover-action .mui-table-view {
    margin: 0;
    text-align: center;
    color: #999;
    border-radius: 0px;
}
.mui-input-row label{
  line-height: 2rem;
  padding: 0px;
}
.mui-radio input[type='radio']:before, .mui-checkbox input[type='checkbox']:before{
  margin-top: 0.3rem;
    display: block;
}
