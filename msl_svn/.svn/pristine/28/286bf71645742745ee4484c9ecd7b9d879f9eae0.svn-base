/*密码*/


.mui-content{
  position: fixed;
  width: 100%;
  bottom: 0px;
}
table#number_input {
  background-color: white;
  width: 100%;
  text-align: center;

}

table#number_input td {
  border: 1px solid gainsboro;
  border-right: 0;
  width: 10%;
  height: 40px;
  padding-top: 0.7rem;
}

table#number_input td:last-child {
  border-right: 1px solid gainsboro;
}

table#keyboard {
  /*position: fixed;*/
  bottom: 0;
  left: 0;
  right: 0;
  text-align: center;
  width: 100%;
  height: 160px;
  background-color: white;
}

table#keyboard td {
  border: 1px solid gainsboro;
  border-right: 0;
  border-bottom: 0;
  width: 33%;
  height: 40px;
}

table#keyboard td:enabled {
  color: red;
}

table#keyboard td:last-child {
  border-right: 1px solid gainsboro;
}
