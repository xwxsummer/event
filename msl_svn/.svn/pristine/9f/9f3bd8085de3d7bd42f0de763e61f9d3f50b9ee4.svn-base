<div class="box ">
    <div class="mui-scroll-wrapper">
        <div class="mui-scroll">
    <div class="box2 posi_relative back_fff" *ngFor="let info of data">

        <div class="box3">
          <div class="system">
          <div class="system_time text_center">{{info?.create_time}}</div>
          <div class="systemUpdate text_left back_fff" (click)="sytemInfo(info?.content,info?.create_time)">
            <div class="b-b fonSml col_333">{{info?.title}}</div>
            <div class="">查看更多<span class="mui-icon mui-icon-arrowright pull-right m-t10"></span></div>
          </div>
          </div>  
        </div>
    </div>
    <div *ngIf="uploading.noneIfno==1" style="margin-bottom: 35px;">
    <div class="text_center fon85" *ngIf="!uploading.isLoading" id="loadingMore">{{uploading.loadingMore}}</div>
    <div class="text_center fon85 moreInfo" *ngIf="uploading.isLoading">正在加载 <img src="../../../assets/images/loading.jpg" alt=""/></div>
      
    </div>
    <div class="text_center m-t3 zanwu" *ngIf="uploading.noneIfno==2">暂无信息</div>
    </div>
    </div>
