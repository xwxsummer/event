.box{
  width: 100%;
  height: 100%;
}
.box .box2{
  height: 20%;
  background: #fff;
}
.box .box2 span{
  display: block;
  text-align: center;

}
.box .box2 span:nth-child(1){
  padding-top: 2rem;

}
.box .box2 span:nth-child(2){
  color: #000;
  font-weight: 600;


}
.box .box2 span:nth-child(3){
  color: #ea5529;

}
.box .box3{
  height: 2.5rem;
  background: #fff;
  line-height: 2.5rem;
}
.box .box3 span:nth-child(1){
  float: left;
  padding-left: 1rem;
}
.box .box3 span:nth-child(2){
  float: right;
  padding-right: 1rem;
}
#input{
  width: 50%;
background: #ea5529;
display: block;
color: #fff;
border: 0;
}
