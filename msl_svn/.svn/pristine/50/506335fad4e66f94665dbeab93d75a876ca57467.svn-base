.blooming-menu__item-btn {
  transition: box-shadow .28s cubic-bezier(.4, 0, .2, 1), opacity .28s cubic-bezier(.4, 0, .2, 1);
  background-size: 50%;
  background-position: center center;
  background-repeat: no-repeat;
  opacity: 0.8;
}

.blooming-menu__container {
  z-index: 1000;
  position: absolute;
  left: 48.2%!important;
  top: 18.5%!important;
}

.blooming-menu__main {
  width: 4rem!important;
  height: 4rem!important;
  background-color: #ea5529!important;
}

.blooming-menu__main-content {
  font-size: 1rem!important;
}

.blooming-menu__item-btn-wrapper {
  width: 58px!important;
  height: 58px!important;
  /*background-color: #ea5529!important;*/
}

.blooming-menu__item-btn {
  width: 58px!important;
  height: 58px!important;
}

.blooming-menu__item {
  position: absolute!important;
  bottom: 9px!important;
  top: 2px!important;
  left: 1px!important;
}

.blooming-menu__main.is-active {
  transform: rotate(0deg)!important;
}

.blooming-menu__item:nth-of-type(1) .blooming-menu__item-btn {
  background-image: url(assets/images/1.svg);
  background-size: 100% 100%;
}
.blooming-menu__item:nth-of-type(2) .blooming-menu__item-btn {
  /*background-color: #33cccc!important;*/
  background-image: url(assets/images/2.svg);
  background-size: 100% 100%;
}
