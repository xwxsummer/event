/* css css rest*/

html, body, div, h1, h2, h3, h4, h5, h6, p, address, dl, dt, dd, ol, ul, li, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, menu, nav, output, section, time, mark, audio, video, progress {
  margin: 0;
  padding: 0;
}

html, body {
  width: 100%;
  height: 100%;
}

input {
  font-size: 0.9rem;
}

body {
  font-size: 10px;
  color: #999;
  font-family: "Microsoft Yahei", sans-serif;
  background-color: #efeff4;
}

a {
  text-decoration: none;
  color: #333;
}

.f9f9fb {
  background-color: #f9f9fb;
}

a:hover {
  text-decoration: none;
}

ul, ol {
  list-style: none;
}

input {
  border: 0 none;
  outline: none;
}


