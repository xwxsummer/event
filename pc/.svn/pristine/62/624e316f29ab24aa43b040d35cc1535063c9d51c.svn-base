.cardBotton{
    float:left;
    border: 1px solid #e5e5e5;
    width: 150px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    margin-left:20px;
    border-radius: 5px;
}
.brImg{
    margin-bottom:20px;
}
.brImg > img{
    border: 1px solid #e5e5e5; 
    width:138px;
    height: 33px;
    box-shadow: 5px 5px 2px #888888;
    margin-right: 10px;
}
#inp{
    border-radius: 10px;
    width:200px;
    height: 40px;
    border:1px solid #e5e5e5;
    padding-left: 4px;
}
.complete{
    width:200px;
    height: 50px;
    line-height: 50px;
    background-color: #e85529;
    text-align: center;
    color:white;
    margin-left:2%;
    border-radius: 5px;
    font-size:16px;
}
.btn-lg{
    background-color: #e85529;
    color: white;
    border: none; 
    width:140px;
}
.maslai{
    margin-top:20px;
    margin-bottom: 20px;
    font-size: 16px;
}