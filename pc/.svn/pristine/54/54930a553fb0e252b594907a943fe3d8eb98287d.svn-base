*{
    margin:0;
    padding:0;
}
.paging{
    width:98%;
    height: 5%;
    background-color: #f3f6f8;
    margin-left: 1%;
    border: 1px solid #ccc; 
    border-top:none;
    text-align: center;
    padding-top: 1%;
}
.title{
    height: 90px;
    line-height: 90px;
    padding-left: 40px;
}
.list{
    width:98%;
    margin-left:1%;
    margin-top:1%;
    border-color:#e2e8eb;
}
.search{
    height: 8rem;
    margin-left: 40px;
    width:96%;
    margin-top:1%;
    border-color:#e2e8eb;
}
.search tr td input{
    border-radius: 5px;
    display: inline-block;
    width: 50%;
    /*height: 2rem;
    border:1px solid #afafaf;
    background-color:transparent;*/
}
.select{
    border-radius: 5px;
    display: inline-block;
    width: 40%;
    font-size:8px;
}
.button{
    width:120px;
    background-color: #eb5529;
    border-radius: 5px;
    border:none;
    color:#fff;
}